{
  "extends": ["standard", "standard-react"],
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "no-multiple-empty-lines": [
      "error",
      {
        "max": 4
      }
    ],
    "react/prop-types": 0,
    "no-alert": 1,
    "camelcase": 0
  }
}
